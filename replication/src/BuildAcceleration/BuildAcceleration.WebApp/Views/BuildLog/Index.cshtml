@{
}

@{
    var allocation = ViewBag.allocations[0];
}

<p>Commit: <a href="@ViewBag.CommitUrl" target="_blank">@ViewBag.VcsRevision</a></p>

<details>
    <summary>@((allocation as IAsyncGrouping<string, object>).Key as string)</summary>
    @foreach (var action in allocation)
    {
        <details>
            <summary>@action.StepName @action.RunTime</summary>
            <pre><code>@action.Output</code></pre>
        </details>
    }
</details>
